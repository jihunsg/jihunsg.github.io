package org.javaro.lecture;

public class Ex5_5 {
	public static void main(String[] args) {
		System.out.println("18김지훈 20191010 소수찾기");
		int max = 30; int k;
		for(int i=2;i<=max;i++) {
			k = 0;
			for(int j = 2;j<i;j++) {
				if(i%j == 0) {
					k =1;
				}
				continue;
			}
			if(k == 0)
				System.out.print(i+" ");
		}
		
		
	}
}
